# MongoDB connection string (replace <user>, <pass>, <cluster>)
MONGODB_URL=mongodb+srv://<user>:<pass>@cluster.mongodb.net/gamezone

# Server port
PORT=3001

# JWT secrets (use secure random values in production)
ACCESS_TOKEN_SECRETE=replace_with_strong_random_value
REFRESH_TOKEN_SECRETE=replace_with_strong_random_value

# Mailtrap token (for development email delivery)
MAILTRAP_TOKEN=your_mailtrap_token_here
